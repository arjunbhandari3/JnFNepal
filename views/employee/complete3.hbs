    {{/extend}}

    {{#extend "body"}}
    <section class="section">
        <div class="title">Upload Profile Picture</div>
        <div class="container content">
            <div class="">

                <form class="fifty form" action="" method="post" enctype="multipart/form-data">
                    <label for="profile_pic">
                        <div>Pick your profile picture (JPG, PNG):</div>
                        <input type="file" name="myFile" style="border: none; padding: 0;">
                    </label>
                    <button class="btn">Complete Profile</button>
                </form>

            </div>
        </div>
    </section>
    {{/extend}}